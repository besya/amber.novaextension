<completions>
  <provider>
    <syntax>amber</syntax>
    <exclude-selector>string, comment</exclude-selector>
    <set>amber.std.fs</set>
  </provider>
  <set name="amber.std.fs" symbol="function">
    <completion string="dir_create">
      <behavior>
        <append>($[path])</append>
      </behavior>
    </completion>
    <completion string="dir_exists">
      <behavior>
        <append>($[path])</append>
      </behavior>
    </completion>
    <completion string="escape_non_glob_chars">
      <behavior>
        <append>($[path])</append>
      </behavior>
    </completion>
    <completion string="file_append">
      <behavior>
        <append>($[path], $[content])</append>
      </behavior>
    </completion>
    <completion string="file_chmod">
      <behavior>
        <append>($[path], $[mode])</append>
      </behavior>
    </completion>
    <completion string="file_chown">
      <behavior>
        <append>($[path], $[user])</append>
      </behavior>
    </completion>
    <completion string="file_exists">
      <behavior>
        <append>($[path])</append>
      </behavior>
    </completion>
    <completion string="file_extract">
      <behavior>
        <append>($[path], $[target])</append>
      </behavior>
    </completion>
    <completion string="file_glob">
      <behavior>
        <append>($[path])</append>
      </behavior>
    </completion>
    <completion string="file_glob_all">
      <behavior>
        <append>($[paths])</append>
      </behavior>
    </completion>
    <completion string="file_read">
      <behavior>
        <append>($[path])</append>
      </behavior>
    </completion>
    <completion string="file_write">
      <behavior>
        <append>($[path], $[content])</append>
      </behavior>
    </completion>
    <completion string="symlink_create">
      <behavior>
        <append>($[origin], $[destination])</append>
      </behavior>
    </completion>
  </set>
</completions>
